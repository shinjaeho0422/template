    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.9",
    "multer": "^1.4.2",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.1",
    "saslprep": "^1.0.3",
    "supports-color": "^7.2.0"
  },
